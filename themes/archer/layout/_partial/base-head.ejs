  <script src="https://www.gstatic.com/firebasejs/5.7.1/firebase.js"></script>
  <script>
    // Initialize Firebase
    var config = {
      apiKey: 'AIzaSyC2_L1EsDollJr9B3Q2EW6RbHMCUOECjuQ',
      authDomain: 'myblog-e0d34.firebaseapp.com',
      databaseURL: 'https://myblog-e0d34.firebaseio.com',
      projectId: 'myblog-e0d34',
      storageBucket: 'myblog-e0d34.appspot.com',
      messagingSenderId: '633925896696'
    }
    firebase.initializeApp(config)

    function sendToken(token) {
      new Fingerprint2().get(function(result) {
        firebase
        .database()
        .ref('users/' + result)
        .set({
          token
        })
      })
    }

    var messaging = firebase.messaging()
    var database = firebase.database()
    messaging.usePublicVapidKey(
      'BNeTtxbWEu89hWH5rlQp64gSVnTnGTdj6nPZFnsT-zDtG-Kt0_aEEJbG0eD11uuP6jHvPSMxOYbZWRyT2xnCdG0'
    )
    messaging
      .requestPermission()
      .then(function() {
        console.log('Notification permission granted.')
        messaging
          .getToken()
          .then(function(currentToken) {
            if (currentToken) {
              sendToken(currentToken)
            } else {
              // Show permission request.
              console.log(
                'No Instance ID token available. Request permission to generate one.'
              )
            }
          })
          .catch(function(err) {
            console.log('An error occurred while retrieving token. ', err)
          })
      })
      .catch(function(err) {
        console.log('Unable to get permission to notify.', err)
      })
    // Callback fired if Instance ID token is updated.
    messaging.onTokenRefresh(function() {
      messaging
        .getToken()
        .then(function(refreshedToken) {
          sendToken(refreshedToken)
        })
        .catch(function(err) {
          console.log('Unable to retrieve refreshed token ', err)
        })
    })
  </script>
